			link: ':Cms:Images:default'
			icon: fa-file-image-o
			category: Content
			name: Photo
			description: 'Manage images, edit description...'
			priority: 15
		]]


	cms.presenters.modules:
		class: Devrun\CmsModule\Presenters\ModulePresenter
		tags: [devrun.presenter, administration: [
			link: ':Cms:Module:default'
			icon: fa-sitemap
			category: Content
			name: Modules
			description: 'Manage modules...'
			priority: 25
		]]

	cms.presenters.domains:
