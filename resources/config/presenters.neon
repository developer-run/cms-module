	cms.presenters.log:
		class: Devrun\CmsModule\Presenters\LogPresenter
		tags: [devrun.presenter, administration: [
			link: ':Cms:Log:default'
			icon: fa-html5
			category: Content
			name: Log
			description: 'Manage logs...'
			priority: 5
		]]

	cms.presenters.translate:
		class: Devrun\CmsModule\Presenters\TranslatePresenter
		tags: [devrun.presenter, administration: [
			link: ':Cms:Translate:default'
			icon: fa-language
			category: Content
			name: Translate
			description: 'Manage texts, edit content...'
			priority: 10
		]]

	cms.presenters.images:
		class: Devrun\CmsModule\Presenters\ImagesPresenter
