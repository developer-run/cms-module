				<li class="active">{_admin.page.edit}</li>
			</ol>
		</section>

		<!-- Main content -->
		{snippet flash}
		{control flashMessageControl}
		{control flashMessageControl:toast}
		{/snippet}
		<section n:snippet="content" class="content">
			{include content}
		</section>
		{control modalActionControl}
	</div>
	<!-- /.content-wrapper -->

	{include "#footer.latte"}

	<!-- Control Sidebar -->
