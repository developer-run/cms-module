{
  "name": "devrun-cms",
  "description": "Developer Run CMS",
  "version": "0.1.1",
  "keywords": ["nette", "devrun", "cms"],
  "author": "Pavel Paulik",
  "dependencies": {
    "grunt-sync": "^0.5.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-inline-urls": "^1.2.0",
    "less-plugin-group-css-media-queries": "^1.1.1"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-compress": "^1.4.1",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-less": "^1.0.0",
    "grunt-contrib-uglify": "^2.2.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-dev-update": "^1.4.0",
    "grunt-multi-dest": "^1.0.0",
    "grunt-nette-basepath": "^0.3.1",
    "grunt-shell": "^1.1.2",
    "grunt-usemin": "^2.4.0"
  },
  "modules": {
    "less": {
      "development": {
        "files": {
          "app/modules/front-module/resources/build/css/index.css": "app/modules/front-module/Resources/src/less/index.less",
          "app/modules/front-module/resources/build/css/bootstrap.css": "app/modules/front-module/Resources/src/less/bootstrap.less"
        }
      },
      "production": {
        "files": {
          "app/modules/front-module/resources/build/css/cms-index.min.css": "app/modules/front-module/Resources/src/less/index.less"
        }
      }
    },
    "useminPrepare": {
      "files" : [
        "app/modules/front-module/src/FrontModule/Presenters/templates/@layout.latte",
        "vendor/devrun/cms-module/src/Devrun/CmsModule/Presenters/templates/@layout.latte"
      ]
    },
    "watch": {
      "less": {
        "files" : ["vendor/devrun/cms-module/Resources/src/less/**/*.less"]
      }
    }
  }
}
