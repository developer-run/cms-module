{
  "name": "devrun/cms-module",
  "description": "DevRun Cms module",
  "keywords": ["cms", "nette", "devrun", "module"],
  "version": "v0.8.3",
  "minimum-stability": "dev",
  "homepage": "http://devrun.cz",
  "license": "proprietary",
  "authors": [
    {
      "name": "Pavel Paul√≠k"
    }
  ],
  "require": {
    "devrun/framework": "^0.8",
    "devrun/ckeditor-module": "v4.*",
    "devrun/doctrine-module": "v3.3.*",
    "phpzip/phpzip": "^2.0",
    "ublaboo/datagrid": "^5.5",
    "wa72/htmlpagedom": "^1.3"
  },
  "require-dev": {
    "dg/adminer-custom": "~1.6",
    "phpunit/phpunit": "^5.0",
    "phpunit/phpunit-dom-assertions": "^2.0",
    "contributte/codeception": "^0.7.7"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/developer-run/framework"
    },
    {
      "type": "vcs",
      "url": "https://github.com/developer-run/doctrine-module"
    },
    {
      "type": "vcs",
      "url": "https://github.com/developer-run/ckeditor-module"
    }
  ],
  "autoload": {
    "classmap": ["src/"]
  },
  "config": {
    "vendor-dir": "../../../vendor"
  },

  "extra": {
    "branch-alias": {
      "dev-master": "0.8-dev"
    },
    "devrun": {
      "installers" : ["Devrun\\Module\\Installers\\MigrationInstaller"],
      "configuration": {
        "extensions": {
          "cms": "Devrun\\CmsModule\\DI\\CmsExtension"
        }
      }
    }
  }
}